{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "start": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["./dist", "./tsconfig.tsbuildinfo"]
    },
    "ts-check": {
      "outputs": ["./tsconfig.tsbuildinfo"]
    }
  }
}
